{
    "name": "prestamodule/slack-laravel",
    "description": "Laravel 4 and 5 integration for the razorpay/slack package, including facades and service providers.",
    "keywords": ["slack", "laravel"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "maknz",
            "email": "github@mak.geek.nz"
        },
        {
            "name": "Team Razorpay",
            "email": "support@razorpay.com"
        },
        {
            "name": "PrestaModule",
            "email": "github@presta-module.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "razorpay/slack": "1.10.1"
    },
    "repositories": [
    ],
    "autoload": {
        "psr-4": {
            "Razorpay\\Slack\\Laravel\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "aliases": {
                "Slack": "Razorpay\\Slack\\Laravel\\Facade"
            }
        }
    }
}
